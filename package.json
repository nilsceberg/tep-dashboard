{
  "name": "tep_dashboard",
  "version": "0.0.1",
  "description": "Dashboard components for the Tool Evaluation Platform.",
  "repository": {
    "type": "git",
    "url": "git://github.com/wasp-sweden/tep-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/wasp-sweden/tep-dashboard/issues"
  },
  "homepage": "https://github.com/wasp-sweden/tep-dashboard",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:ts": "tsc",
    "build:backends": "dash-generate-components ./src/lib/components tep_dashboard -p package-info.json --r-prefix '' --jl-prefix '' --ignore \\.test\\. --ignore src/lib/components/visualization/depclean/private",
    "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
    "build": "npm run build:ts && npm run build:js && npm run build:backends",
    "build:activated": "npm run build:ts && npm run build:js && npm run build:backends-activated"
  },
  "author": "Milla Widell <millawidell@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@types/d3": "^7.1.0",
    "@types/uuid": "^8.3.4",
    "antd": "^4.18.7",
    "d3": "^7.3.0",
    "ramda": "^0.26.1",
    "react-grid-layout": "^1.3.1",
    "react-router-dom": "^6.2.1",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@plotly/dash-component-plugins": "^1.2.0",
    "@plotly/webpack-dash-dynamic-import": "^1.2.0",
    "@types/lodash": "^4.14.178",
    "@types/react-dom": "^17.0.11",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.1.1",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-docgen": "^4.1.1",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "styled-jsx": "^3.2.1",
    "terser-webpack-plugin": "^2.3.0",
    "tsc": "^2.0.4",
    "typescript": "^4.5.5",
    "webpack": "4.36.1",
    "webpack-cli": "3.3.6",
    "webpack-serve": "3.1.0"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
